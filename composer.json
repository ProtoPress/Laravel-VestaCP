{
  "name": "madeitbelgium/laravel-vestacp-sdk",
  "version": "0.0.1",
  "description": "PHP Laravel VestaCP SDK",
  "keywords": ["hosting", "vestacp"],
  "homepage": "http://www.madeit.be",
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Tjebbe Lievens - Made I.T.",
      "homepage": "https://www.madeit.be",
      "email": "tjebbe.lievens@madeit.be"
    }
  ],
  "require": {
    "illuminate/support": "5.*",
    "illuminate/container": "5.*",
    "illuminate/validation": "5.*",
    "guzzlehttp/guzzle": "6.3.*",
    "php": ">=5.6"
  },
  "require-dev": {
      "phpunit/phpunit": "7.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
      "psr-4": {
          "MadeITBelgium\\VestaCP\\": "src/"
      }
  },
  "extra": {
       "laravel": {
           "providers": [
               "MadeITBelgium\\VestaCP\\VestaCPServiceProvider"
           ]
       }
    }
}
